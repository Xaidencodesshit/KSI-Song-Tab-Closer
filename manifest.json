{
    "manifest_version": 3,
    "name": "KSI Song Tab Closer",
    "version": "1.0",
    "description": "Closes the tab when KSI's new song is detected.",
    "permissions": ["tabs", "activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icons/icon48.png"
    }
  }
  